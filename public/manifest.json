{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "VCT Tools",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.199.0.15",
    "author": "infinity-atom",
    "icon": "icon-hover.png",
    "icon_gray": "icon-idle.png",
    "description": "VCT Tools for Desktop"
  },
  "data": {
    "start_window": "MainWindow",
    "windows": {
      "MainWindow": {
        "file": "index.html",
        "transparent": false,
        "resizable": true,
        "use_os_windowing": true,
        "size": {
          "width": 1000,
          "height": 800
        },
        "min_size": {
          "width": 800,
          "height": 600
        }
      }
    }
  }
}
